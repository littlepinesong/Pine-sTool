<Window x:Class="PineProject.PineMusicCloud"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PineProject"
        mc:Ignorable="d"
        Title="PineMusicCloud" Height="300" Width="300">
    <Grid>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="155,92,0,0" VerticalAlignment="Top" Width="75"/>
        <Frame x:Name="frame" Content="Frame" HorizontalAlignment="Left" Height="100" Margin="82,92,0,0" VerticalAlignment="Top" Width="100"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="100" Margin="121,82,0,0" VerticalAlignment="Top" Width="100"/>
        <Menu x:Name="menu" HorizontalAlignment="Left" Height="100" Margin="115,35,0,0" VerticalAlignment="Top" Width="100"/>
        <RichTextBox x:Name="richTextBox">
            <FlowDocument>
                <Paragraph>
                    <Run Text="RichTextBox"/>
                </Paragraph>
            </FlowDocument>
        </RichTextBox>

    </Grid>
</Window>
